  var { threadID,messageID } = event;
  if (event.body.index0f("Hi")==0 ||(event.body.index0f("Nugagawen")==0 ||(event.body.index0f("oo nalng")==0 ||(event.body.index0f("ampogi ko talaga")==0)))) {
